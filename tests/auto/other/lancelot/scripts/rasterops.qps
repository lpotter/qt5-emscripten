setCompositionMode SourceOver
translate 250 45
drawText 20 0 "Or ROP"
translate 0 50
drawText 20 0 "And ROP"
translate 0 50
drawText 20 0 "Xor ROP"
translate 0 50
drawText 20 0 "Nor ROP"
translate 0 50
drawText 20 0 "Nand ROP"
translate 0 50
drawText 0 0 "NSrcXorDst ROP"
