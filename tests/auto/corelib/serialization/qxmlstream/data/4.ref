StartDocument( documentVersion="1.0" )
DTD( text="
<!DOCTYPE doc [
<!ELEMENT doc (#PCDATA)>
<!ENTITY % e SYSTEM "something">
<!ENTITY replace "replace">
<!ENTITY me "me">
%e;
]>" dtdName="doc"
